<Window x:Class="TelegramObserverDesktop.CodeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TelegramObserverDesktop"
        mc:Ignorable="d"
        Title="Введите код" Height="110" Width="300">
    <Grid>
        <TextBox Name="tbCode"  Margin="10,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Button Content="Ок" Margin="10,33,10,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
